<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace = "login">
	
	<!-- 회원가입 -->
	<insert id = "insertLogin" parameterType = "log.dto.LoginDTO">
		<![CDATA[
			INSERT INTO seoulmembers(memId, memPw, memName, memNick, memProfile,memPhone)
			VALUES (#{memId}, #{memPw}, #{memName}, #{memNick}, #{memProfile}, #{memPhone})
		]]>
	</insert>
	
	
	<select id = "selectById" parameterType = "string" resultType="log.dto.LoginDTO">
		SELECT * FROM seoulmembers
		WHERE memId = #{memId}
	</select>
	
	
	</mapper>
	